{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "rxjs": [
        "./src"
      ],
      "rxjs/*": [
        "./src/*"
      ]
    },
    "outDir": "./tmp/tests/rxjs",
  },
  "include": [
    "./src/index.ts",
    "./src/operators.ts",
    "./src/testing.ts",
    "./src/**/*-spec.ts",
  ],
  "exclude": [
    "./package"
  ]
}
